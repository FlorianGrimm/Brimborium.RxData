{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "brimborium-rxdata": {
      "projectType": "library",
      "root": "projects/brimborium-rxdata",
      "sourceRoot": "projects/brimborium-rxdata/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "configurations": {
            "production": {
              "tsConfig": "projects/brimborium-rxdata/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/brimborium-rxdata/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/brimborium-rxdata/tsconfig.spec.json"
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "1982b805-110b-42ff-910c-d699b44d8ad8"
  }
}
